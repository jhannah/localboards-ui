<h1 data-bind="text: board.title">Viewing board</h1>
<br />
<div class="badge-container">
  <div class="row">
    <ul class="thumbnails list-unstyled">
      <li class="span3">
        <div class="thumbnail" style="padding: 0">
          <div style="padding:4px">
            <img alt="300x200" style="width: 100%" src="http://placehold.it/200x150">
          </div>
          <div class="caption">
            <p data-bind="text: board.duties">This description</p>
          </div>
          <div class="modal-footer" style="text-align: left">
            <div class="row-fluid">
              <div class="span4"><b data-bind="text: board.size">1</b><br/><small>SEATS</small></div>
              <div class="span4"><b data-bind="text: board.openSeats">2</b><br/><small>OPEN</small></div>
            </div>
          </div>
        </div>
      </li>
    </ul>
  </div>
</div>
<div class="mission">
  <h3>Mission</h3>
  <span data-bind="text: board.missionStatement">Here's our mission statement</span>
</div>
<div class="orgInfo">
  <h3>Meeting Info</h3>
  Meeting dates: <span data-bind="text: board.meetingDates">N/A</span><br />
  Meeting place: <span data-bind="text: board.meetingPlace">N/A</span><br />
  Meeting times: <span data-bind="text: board.meetingTime">N/A</span><br />
</div>
<div class="members">
  <h3>Members</h3>
  <div data-bind="foreach: board.members">
    <div class="row">
    <ul class="thumbnails list-unstyled">
      <li class="span3">
        <div class="thumbnail" style="padding: 0">
          <div style="padding:4px">
            <img alt="100x150" style="width: 100%" src="http://placehold.it/100x150">
          </div>
          <div class="caption">
            <h2 data-bind="text: name">Name</h2>
            <p>This description</p>
          </div>
          <div class="modal-footer" style="text-align: left">
            <div class="row-fluid">
            </div>
          </div>
        </div>
      </li>
    </ul>
  </div>
  </div>
</div>
