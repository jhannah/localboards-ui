<h1>View boards</h1>
<br />
<div class="row">
  <ul class="thumbnails list-unstyled" data-bind="foreach: boards()">
    <li class="span3">
      <div class="thumbnail" style="padding: 0">
        <div style="padding:4px">
          <img alt="300x200" style="width: 100%" src="http://placehold.it/200x150">
        </div>
        <div class="caption">
          <h2 data-bind="text: title">Name</h2>
        </div>
        <div class="modal-footer" style="text-align: left">
          <div class="row-fluid">
            <div class="span4"><b data-bind="text: size">1</b><br/><small>SEATS</small></div>
            <div class="span4"><b data-bind="text: openSeats">2</b><br/><small>OPEN</small></div>
          </div>
        </div>
      </div>
    </li>
  </ul>
</div>